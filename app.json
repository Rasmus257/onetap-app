{
  "expo": {
    "name": "One Tap",
    "slug": "One Tap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/natively-dark.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/natively-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Natively",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "entitlements": {
        "aps-environment": "development",
        "com.apple.security.application-groups": [
          "group.com.anonymous.Natively.onesignal"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/natively-dark.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.Natively"
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ],
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "onetap",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "oneSignalAppId": "84bd5859-82bd-44dd-8a2e-c7e1ccca59f9",
      "backendUrl": "https://f9sn2jgyaeehkkpq9b36q2jt67yk4bmc.app.specular.dev"
    }
  }
}